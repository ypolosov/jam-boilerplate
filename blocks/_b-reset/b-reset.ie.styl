@import '../config';

audio,
canvas,
video {
    *display: inline;
    *zoom: 1;
}


/* Embedded content */

img {
    -ms-interpolation-mode: bicubic;
}

legend {
    *margin-left: -7px;
}

button,
input,
select,
textarea {
    *vertical-align: middle;
}

button,
input[type="button"],
input[type="reset"],
input[type="submit"] {
    *overflow: visible;
}

input[type="checkbox"],
input[type="radio"] {
    *width: 13px;
    *height: 13px;
}